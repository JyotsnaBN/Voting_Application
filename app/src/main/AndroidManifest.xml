<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myvotingapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyVotingApplication">
        <activity android:name=".View_Result" />
        <activity
            android:name=".Voting"
            android:label="@string/title_activity_voting"
            android:theme="@style/Theme.MyVotingApplication.NoActionBar" />
        <activity
            android:name=".Voter_Login"
            android:label="@string/title_activity_voter__login"
            android:theme="@style/Theme.MyVotingApplication.NoActionBar" />
        <activity android:name=".Admin_Info" />
        <activity
            android:name=".Admin_Login"
            android:label="@string/title_activity_admin__login"
            android:theme="@style/Theme.MyVotingApplication.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>